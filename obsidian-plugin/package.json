{
  "name": "theophysics-research-assistant",
  "version": "1.0.0",
  "description": "Semantic markup, math translation, and AI-powered research assistance for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "semantic-markup",
    "math-translation",
    "research",
    "theophysics"
  ],
  "author": "Theophysics Research Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "obsidian": "latest",
    "tslib": "^2.6.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "pg": "^8.11.0",
    "csv-parse": "^5.5.0",
    "uuid": "^9.0.0"
  }
}
